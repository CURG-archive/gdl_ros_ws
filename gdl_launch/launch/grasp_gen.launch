<launch>

     <!--
        Used to map uvd to xyz in order to convert uvd hand positions from the deep learning algorithm to actual xyz coordinates
        This node requires camera_info topic
     -->
    <node name="uvd_to_xyz_service_node" pkg="uvd_xyz_conversion" type="main.py"/>

    <!--This node must run on a machine with a proficiently powerful gpu-->
    <node name="ros_grasp_server" pkg="pylearn2_classifier_gdl" type="ros_grasp_server.py"/>

</launch>